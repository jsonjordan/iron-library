<body class="UserIndex">

  <h2>
    Current Users
    <span class="badge" id="user-count"></span>
  </h2>

  <table class="table table-hover table-striped table-condensed">
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Campus</th>
        <th>Class</th>
        <th>Slack Name</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
    <% @users.each do |u| %>
      </tr>
        <th><%= u.name %></th>
        <th><%= u.email %></th>
        <th><%= u.campus.city %></th>
        <th><%= u.klass %></th>
        <th><%= u.slack_name %></th>
      </tr>
      <% end %>
      </tbody>
  </table>

</body>


